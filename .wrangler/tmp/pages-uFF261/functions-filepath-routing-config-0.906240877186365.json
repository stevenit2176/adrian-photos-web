{
  "routes": [
    {
      "routePath": "/api/auth/login",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/login.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/auth/logout",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/logout.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/auth/me",
      "mountPath": "/api/auth",
      "method": "GET",
      "module": [
        "api/auth/me.ts:onRequestGet"
      ]
    },
    {
      "routePath": "/api/auth/refresh",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/refresh.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/auth/register",
      "mountPath": "/api/auth",
      "method": "POST",
      "module": [
        "api/auth/register.ts:onRequestPost"
      ]
    },
    {
      "routePath": "/api/products/pricing",
      "mountPath": "/api/products",
      "method": "GET",
      "module": [
        "api/products/pricing.ts:onRequestGet"
      ]
    },
    {
      "routePath": "/api/categories",
      "mountPath": "/api/categories",
      "method": "GET",
      "module": [
        "api/categories/index.ts:onRequestGet"
      ]
    },
    {
      "routePath": "/",
      "mountPath": "/",
      "method": "",
      "middleware": [
        "_middleware.ts:onRequest"
      ]
    }
  ],
  "baseURL": "/"
}